{
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": "trybe-backend",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "complexity": ["error", 10],
      "max-lines-per-function": ["error", 50],
      "indent": ["error", 2],
      "no-trailing-spaces": ["error", { "skipBlankLines": false }],
      "quotes": [2, "single"],
      "no-undef": "off",
      "camelcase": "off"
    }
}
